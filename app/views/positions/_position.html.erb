<div class="card">
  <div class="card-header">
    <h1 class="card-title"><%= position.coin.name %></h1>
  </div>
  <div class="card-body">
    <p>Quantity: <%= position.quantity %></p>
    <p>Value: $<%= position.value %></p>
  </div>
  <div class="card-footer">
    <img src="<%= position.coin.img_url %>">
  </div>
</div>
