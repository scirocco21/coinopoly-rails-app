<% @users.each do |user| %>
  <% if current_user != user %>
    <%= link_to user.name, user_path(user) %><br>

    <% if current_user.following?(user) %>
      <%= render partial: "unfollow", locals: {user: user} %><br>
      <% else %>
      <%= render partial: "follow", locals: {user: user} %><br>
    <% end %>
  <% end %>
<% end %>
