<% if flash[:notice] %>
  <%= flash[:notice] %><br>
<% end %>

<% @users.each do |user| %>
  <%= user.name %><br>

  <% if current_user.following?(user) %>
    <%= render partial: "unfollow", locals: {user: user} %><br>
  <% else %>
    <%= render partial: "follow", locals: {user: user} %><br>
  <% end %>
<% end %>
